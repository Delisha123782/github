        "#languages known by peron A\n",
        "a_langs=['malayalam','hindi','english','spanisch','french']\n",
        "\n",
        "#languages known by person B\n",
        "b_langs=['malayalam','french','chinese','english','arabic','hindi']\n",
        "\n",
        "# Convert lists to sets\n",
        "a_langs_set = set(a_langs)\n",
        "b_langs_set = set(b_langs)\n",
        "\n",
        "#languages known by  either  person A or person B\n",
        "all_langs=a_langs_set | b_langs_set\n",
        "print(\"All languages known;\",all_langs)\n",
        "\n",
        "#languages known by both person A and person B\n",
        "common_langs=a_langs_set & b_langs_set\n",
        "print(\"Languages known by both A and B:\",common_langs)\n",
        "\n",
        "#langauges known  by person A not person B\n",
        "a_unique=a_langs_set - b_langs_set\n",
        "print(\"Languages known by A not B:\",a_unique)\n",
        "\n",
        "#langauges known by person B not person A\n",
        "b_unique=b_langs_set - a_langs_set\n",
        "print(\"Languages known by B not A:\",b_unique)"
      ],
