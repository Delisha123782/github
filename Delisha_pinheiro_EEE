        "def is_right(a,b,c):\n",
        "  #first,sort the sides to ensure the longest side is the hypotenuse\n",
        "  sides=[a,b,c]\n",
        "  sides.sort()\n",
        "  #apply pythagorean theorem:a^2+b^2=c^2\n",
        "  return sides[0]**2+sides[1]**2==sides[2]**2\n",
        "side1=int(input(\"Enter the length of side 1:\"))\n",
        "side2=int(input(\"Enter the length of side 2:\"))\n",
        "side3=int(input(\"Enter the length of side 3:\"))\n",
        "if is_right(side1,side2,side3):\n",
        "  print(f\"The triangle of the sides{side1},{side2},{side3} is a right triangle\")\n",
        "else:\n",
        "  print(f\"The triangle of the sides{side1},{side2},{side3} is not a right triangle\")"
      ],
